    "classnames": "^2.3.2",
    "isbot": "^3.6.5",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.2",
    "@remix-run/eslint-config": "^1.8.2",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.25",
